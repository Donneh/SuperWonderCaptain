import tkinter
import os
from tkinter import*

#Tkinter scherm aanroepen
master = Tk()

Label(text="Hints nodig?").pack()

separator = Frame(height=2, bd=1, relief=SUNKEN)
separator.pack(fill=X, padx=5, pady=5)

#Hint button in Tkinter scherm aaroepen
def callback():
    print("click!")

b = Button(master, text="Klik hier!", command=callback)
b.pack()

mainloop()
